<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project_orderfood">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MapActivity"></activity>
        <activity
            android:name=".AboutUsActivity"
            android:label="@string/title_activity_about_us"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ViewDetailOrder"
            android:label="@string/title_activity_view_detail_order"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".User_OrderHistory"
            android:label="@string/title_activity_user__order_history"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".User_OrderCart"
            android:label="@string/title_activity_user__order_cart"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".User_ListFood"
            android:label="@string/title_activity_user__list_cate_food"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".User_ViewDetailFood"
            android:label="@string/title_activity_user__view_detail_food"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".SigninActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>